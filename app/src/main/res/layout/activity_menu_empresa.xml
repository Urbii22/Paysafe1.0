<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuEmpresa">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginStart="29dp"
        android:layout_marginBottom="84dp"
        android:text="Modo Empresa"
        android:textColor="#070707"
        android:textSize="27sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/MetodoCobro"
        android:layout_width="53dp"
        android:layout_height="51dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="84dp"
        android:backgroundTint="#DAE6C7"
        android:contentDescription="@android:string/yes"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintEnd_toStartOf="@+id/CuentaEmpresa"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:srcCompat="@drawable/credit_card_multiple"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/CuentaEmpresa"
        android:layout_width="53dp"
        android:layout_height="51dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="600dp"
        android:contentDescription="Cuenta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/MetodoCobro"
        app:srcCompat="@drawable/iconocuenta"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="204dp"
        android:layout_height="195dp"
        android:layout_marginStart="101dp"
        android:layout_marginEnd="106dp"
        android:layout_marginBottom="89dp"
        app:layout_constraintBottom_toTopOf="@+id/RecibirEmpresa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/logoihm" />

    <Button
        android:id="@+id/RecibirEmpresa"
        android:layout_width="191dp"
        android:layout_height="74dp"
        android:layout_marginBottom="159dp"
        android:backgroundTint="#0F2000"
        android:text="Recibir"
        android:textColor="#D9E5C6"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>